python hazard_curve_converter.py  --input-file=/Users/bmwyss/Documents/GEM/Hazard_Maps/calc_10/hazard_curve/SA-0.2/hazard_curve-smltp_California_C21_Model-gsimltp_Chiou_Youngs_2008.xml --output-file=/Users/bmwyss/Documents/GEM/Hazard_Maps/calc_10/hazard_curve/SA-0.2/hazard_curve-smltp_California_C21_Model-gsimltp_Chiou_Youngs_2008.csv

python parse_loss_curves.py --input-file=/Users/bmwyss/Documents/GEM/loss_curves/loss-curves-555.xml

create table loss_curves4 (poe char(5555), loss char(5555), lat double precision, lon double precision, asset_ref char(55));

copy loss_curves4 from '/Users/bmwyss/Documents/GEM/loss_curves/loss-curves-555-sub2.csv' DELIMITER ',' CSV;

ALTER TABLE loss_curves4 ADD COLUMN geom geometry(POINT,4326);

UPDATE loss_curves4 SET geom = ST_SetSRID(ST_MakePoint(lon,lat),4326);

create table loss_curves_agg as select geom, array_agg(asset_ref) as asset_ref, array_agg(poe) as poes, array_agg(loss) as loss from loss_curves4 group by geom;



