svir metadata:
source = column gx

debt
insert into metadata (name, value) Values ('category', 'economy');
insert into metadata (name, value) Values ('sub_category', 'Economic Resources');
insert into metadata (name, value) Values ('mapped_value', 'Public debt');
insert into metadata (name, value) Values ('type', 'svir');
insert into metadata (name, value) Values ('admin_level', 'national');
insert into metadata (name, value) Values ('date', '2013-09-12');
insert into metadata (name, value) Values ('keyword', 'economy, debt');
insert into metadata (name, value) Values ('source', 'GEM');
insert into metadata (name, value) Values ('application', 'svir-viewer');

